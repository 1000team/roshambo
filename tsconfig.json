{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es2015",
    "module": "commonjs",
    "jsx": "react",
    "jsxFactory": "h",
    "strict": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "skipLibCheck": false,
    "lib": ["dom", "es2015", "es2016", "es2017"]
  },
  "include": ["src"]
}
